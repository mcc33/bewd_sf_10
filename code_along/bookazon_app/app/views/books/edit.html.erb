
<div class="col-es-12">

<h1 style="color:#cc3333"> Edit Book </h1>
<div class="well">
  <%= simple_form_for @book do |f| %>
   <%= f.input :title %>
   <%= f.input :author %>
   <%= f.input :genre %>
   <%= f.input :image_url %>
  <%= f.input :year_published %>
  <%= f.submit "Submit".html_safe, class: 'btn btn-lg btn-danger' %>
  <% end %>
</div>

</div>
