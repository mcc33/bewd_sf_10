<div class="col-12">
<h1>Our Book Collection</h1>
<p>Find me in app/views/books/index.html.erb</p>

<div class="well">
  <% @books.each do |book| %>
  <ul>
    <li> <%= link_to "#{book.title}", book_path(book) %> </li>
  </ul>
  <% end %>
</div>
</div>


<div class="well">
  <%= link_to "Add New Book", new_book_path, class: 'btn btn-default' %>
</div>
